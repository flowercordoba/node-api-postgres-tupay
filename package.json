{
  "name": "api-postgres-tupay",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node src/app.js",
    "start:dev": "nodemon src/app.js",
    "seeder": "npx sequelize-cli db:seed:all --config src/config/config.json --seeders-path src/seeders",
    "reset-db": "npx sequelize-cli db:drop --config src/config/config.json && npx sequelize-cli db:create --config src/config/config.json && npx sequelize-cli db:migrate --config src/config/config.json --migrations-path src/migrations && npx sequelize-cli db:seed:all --config src/config/config.json --seeders-path src/seeders",
    "generateSwagger": "node src/script/generateSwagger.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@faker-js/faker": "^9.0.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.4",
    "sequelize-cli": "^6.6.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  }
